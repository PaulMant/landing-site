{
  "hosting": {
    "public": "dist/landing-site/browser",
    "headers": [{"key": "Cache-Control", "value": "no-cache"}],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
